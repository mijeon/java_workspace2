package db;

import javax.swing.JFrame;
import javax.swing.JTable;

/**/
public class TableTest extends JFrame{
	JTable table;
	
	public TableTest() {
		table=new JTable(2, 2);
		
		add(table);
		
		setSize(600, 500);
		setVisible(true);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
	}
	
	public static void main(String[] args) {
		new TableTest();
	}
}
